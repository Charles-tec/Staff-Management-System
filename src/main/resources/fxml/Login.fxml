<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tec.staffmanagementsystem.controller.LoginController">
   <children>
      <Rectangle fill="DODGERBLUE" height="334.0" layoutX="-98.0" layoutY="-34.0" stroke="#ffffff8b" strokeType="INSIDE" width="588.0" />
      <Label fx:id="lblLogin" layoutX="97.0" layoutY="46.0" text="Login" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="userName" layoutX="98.0" layoutY="92.0" prefHeight="36.0" prefWidth="204.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="98.0" layoutY="141.0" onAction="#login" prefHeight="36.0" prefWidth="204.0" promptText="Password" />
      <Button fx:id="btnLogin" layoutX="215.0" layoutY="193.0" mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="86.0" styleClass="btnGreen" text="Sign In to Administrators" textFill="WHITE">

         <font>
            <Font size="15.0" />
         </font>
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" />
         </effect></Button>
      <Button fx:id="btnLogin1" layoutX="97.0" layoutY="189.0" mnemonicParsing="false" onAction="#login1" prefHeight="36.0" prefWidth="204.0" styleClass="btnGreen" text="Sign In to Engineers" textFill="WHITE">

      <font>
         <Font size="15.0" />
      </font>
      <effect>
         <DropShadow blurType="ONE_PASS_BOX" />
      </effect></Button>
      <Button fx:id="btnLogin2" layoutX="97.0" layoutY="224.0" mnemonicParsing="false" onAction="#login2" prefHeight="1.0" prefWidth="204.0" styleClass="btnGreen" text="Sign In to Students" textFill="WHITE">

         <font>
            <Font size="15.0" />
         </font>
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" />
         </effect></Button>

   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
